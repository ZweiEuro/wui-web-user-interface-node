{
  "name": "@wui/web-user-interface-lib",
  "version": "3.2.1",
  "description": "Renderer side code for the WUI (Web User Interface) library.",
  "bugs": {
    "url": "https://gitlab.zweieuro.at/wui/web-user-interface-node/-/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.zweieuro.at:wui/web-user-interface-node.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dominik Voelkel",
    "email": "voelkel.dominik@gmail.com",
    "url": "https://zweieuro.at"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./types": {
      "types": "./lib/types.d.ts",
      "default": "./lib/types.js"
    },
    "./TestMock": {
      "types": "./lib/TestMock.d.ts",
      "default": "./lib/TestMock.js"
    },
    "./hooks": {
      "types": "./lib/hooks.d.ts",
      "default": "./lib/hooks.js"
    },
    "./replayTool": {
      "types": "./lib/replayTool/replayTool.d.ts",
      "default": "./lib/replayTool/replayTool.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --project buildConfigs/tsconfig.common.json",
    "build:watch": "tsc --project buildConfigs/tsconfig.common.json --watch",
    "clean": "rm -rf ./lib/",
    "genDocs": "typedoc --plugin typedoc-plugin-markdown --out docs src",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "release": "semantic-release",
    "semantic-release": "semantic-release",
    "test": "cross-env NODE_ENV=test jest --config buildConfigs/jest.config.ts",
    "test:ci": "cross-env NODE_ENV=test jest --config buildConfigs/jest.ci.config.ts",
    "test:watch": "cross-env NODE_ENV=test test --watch",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.tsx?": "eslint --cache --cache-location .eslintcache --fix",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@sebbo2002/semantic-release-jsr": "^2.0.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/gitlab": "13.0.2",
    "@semantic-release/npm": "11.0.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.12",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.16",
    "@types/papaparse": "5.3.14",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.10",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "papaparse": "5.4.1",
    "prettier": "3.2.5",
    "prettier-eslint": "16.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "23.0.2",
    "sort-package-json": "2.8.0",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.2",
    "typedoc": "0.25.12",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "papaparse": ">=5.4.1",
    "react": ">=18.2.0"
  },
  "peerDependenciesMeta": {
    "papaparse": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0"
  }
}
